pipeline {
    agent any
    stages {
        stage('Hello') {
            steps {
                echo 'I am inside Hello step'
            }
        }
        stage('Build') {
            steps {
                echo 'I am inside Build step'
            }
        }
        stage('Test') {
            steps {
                echo 'I am inside Test step'
            }
        }
        stage('Release') {
            steps {
                echo 'I am inside Release step'
            }
        }
    }
}
